{
	"manifest_version": 2,
	"name": "Socola Tool",
	"version": "1.0",
	"description": "Code by Socola Đại Ca",
	"browser_action":
	{
		"default_icon": "images/favicon-32x32.png",
		"default_popup": "html/popup.html"
	},
	"icons":
	{
		"16": "images/favicon-32x32.png",
		"32": "images/favicon-32x32.png"
	},
	"permissions": [
		"tabs",
		"contextMenus",
		"http://*/*",
		"https://*/*"
	],
	"background":
	{
		"scripts": [
            "js/jquery.min.js",
			"js/script.js",
			"js/socola-death-click.js"
		]
	},
	"content_scripts": [
	{
		"matches": ["http://auth3.s-wifi.vn/*"],
		"js": ["js/jquery.min.js", "js/auto-connect-free-s-wifi.js"]
	},
	{
		"matches": ["http://online.khoapham.vn/*"],
		"js": ["js/jquery.min.js", "js/remove-share-khoa-phan.js"]
	}]
}
